{
  {% if order_uuid is defined %}"uuid": "{{ order_uuid }}",{% else %}"uuid": "{{ '' | uuid4() }}",{% endif %}
  "type": "Create",
  "resource_name": "{{ resource_name | default('test-allocation') }}",
  "project_slug": "{{ project_slug | default('test-project') }}",
  "customer_slug": "{{ customer_slug | default('test-customer') }}",
  {% if project_uuid is defined %}"project_uuid": "{{ project_uuid }}",{% else %}"project_uuid": "{{ '' | uuid4() }}",{% endif %}
  {% if customer_uuid is defined %}"customer_uuid": "{{ customer_uuid }}",{% else %}"customer_uuid": "{{ '' | uuid4() }}",{% endif %}
  "offering_uuid": "{{ offering_uuid }}",
  {% if marketplace_resource_uuid is defined %}"marketplace_resource_uuid": "{{ marketplace_resource_uuid }}",{% else %}"marketplace_resource_uuid": "{{ '' | uuid4() }}",{% endif %}
  "state": "{{ state | default('executing') }}",
  "offering_type": "{{ offering_type | default('Marketplace.SLURM') }}",
  "created": "{{ created | default('' | timestamp()) }}",
  "attributes": {{ attributes | default('{}') | from_json | to_json }}
}
